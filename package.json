{
  "name": "pandora2",
  "version": "0.0.6",
  "files": [
    "dist",
    "*.d.ts"
  ],
  "repository": "https://github.com/yelingfeng/Pandora2.git",
  "main": "dist/pandora2.umd.js",
  "module": "dist/pandora2.es.js",
  "exports": {
    ".": {
      "import": "./dist/pandora2.es.js",
      "require": "./dist/pandora2.umd.js"
    },
    "./dist/style.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "types": "dist/auto-imports.d.ts",
  "typing": "dist/auto-imports.d.ts",
  "scripts": {
    "dev": "vite --config ./build/base.config.ts",
    "build:docs": "vite build --config ./build/doc.config.ts && node script/copyDir.js",
    "dist": "vite build --config ./build/doc.config.ts && node script/copyDir.js",
    "build:lib": "vue-tsc --noEmit && vite build --config ./build/lib.config.ts",
    "gen": "node ./script/genNewComp/index.js",
    "build:npm": "vite build --config ./build/lib.config.ts",
    "lint": "eslint  src/**/*.{vue,ts}",
    "lint-fix": "yarn lint --fix",
    "commit": "git-cz",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "reinstall": "rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && yarn install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.0.6",
    "@vueuse/core": "^9.1.0",
    "async-validator": "^4.2.5",
    "dayjs": "^1.11.4",
    "element-plus": "2.2.12",
    "lodash-es": "^4.17.21"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.6.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/compiler-sfc": "^3.2.37",
    "axios": "^0.27.2",
    "commitizen": "^4.2.5",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.21.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.3.0",
    "fs-extra": "^10.1.0",
    "gh-pages": "^4.0.0",
    "handlebars": "^4.7.7",
    "husky": "^8.0.1",
    "inquirer": "^9.0.2",
    "jest": "^28.1.3",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "lint-staged": "^13.0.3",
    "np": "^7.6.2",
    "prettier": "^2.7.1",
    "prismjs": "^1.28.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^28.0.7",
    "typescript": "^4.7.4",
    "unplugin-auto-import": "^0.10.3",
    "unplugin-element-plus": "^0.4.1",
    "unplugin-vue-components": "^0.21.2",
    "vite": "^3.0.0",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-dts": "^1.4.1",
    "vite-plugin-md": "0.11.4",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "^2.0.0",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3",
    "vue-tsc": "^0.39.4",
    "vue-types": "^4.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts?(x),json,md,vue}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
